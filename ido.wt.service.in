[Unit]
Description=${PROJECT_NAME} Web Server
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=${CMAKE_INSTALL_PREFIX}/var/www/${PROJECT_NAME}
ExecStart=${CMAKE_INSTALL_PREFIX}/usr/bin/${PROJECT_NAME}.wt
Restart=on-failure
RestartSec=1

[Install]
WantedBy=multi-user.target
